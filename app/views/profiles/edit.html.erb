<% provide(:title, "Edit Profile") %>

<div class="page-header">
  <h2>Edit Profile</h2>
</div>

<div class="row">
  <div class="col-xs-12">
    <%= form_for @profile do |f| %>
      <%= render 'shared/form_errors', object: f.object %>
      <%= render 'form', f: f %>
    <% end %>
  </div>
</div>




